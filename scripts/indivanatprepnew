mri_vol2vol --mov brain.mgz --targ rawavg.mgz --regheader --o brainFSinraw.nii

tkregister2 --mov rawavg.mgz --targ orig.mgz --reg register.native.dat --fslregout register.native.fsl --noedit --regheader


labelconvert ../FS/HCPMMP1out/sub-01/HCPMMP1.nii.gz subcortandglasserCombLUT.txt subcortandglasserCombLUT_final.txt parc.nii

mrconvert brain.mgz brainFSsp.nii -force -nthreads $ncpus

labelsgmfix parc.nii brainFSsp.nii HCPMMP1out/subcortandglasserCombLUT_final.txt parc_fixsubcort.nii -sgm_amyg_hipp -premasked -nthreads 10 -force


mri_vol2vol --mov parc_fixsubcort.nii --targ /mnt/data/alistairp/myconnectome/T1working/FS/sub-01/mri/rawavg.mgz --regheader --no-save-reg --keep-precision --o parc_fixsubcort_innat.nii --nearest




