rm parcnat_fixsubcort.nii

labelsgmfix parcnat.nii sub-01_T1w_brain.nii subcortandglasserCombLUT_final_noH.txt parcnat_fixsubcort.nii -sgm_amyg_hipp -premasked -nthreads $ncpus -force
